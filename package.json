{
  "name": "react_starter_kit",
  "version": "0.2.0",
  "description": "React starter kit with React and React Dom, TailwindCSS, SCSS and Parcel",
  "main": "index.js",
  "scripts": {
    "start": "npm run watch:css && parcel index.html",
    "build": "npm run watch:css && parcel build index.html",
    "build:css": "postcss public/style/style.scss -o public/style/main.css",
    "watch:css": "postcss public/style/style.scss -o public/style/main.css"
  },
  "keywords": [
    "scss",
    "css",
    "tailwind",
    "tailwindcss",
    "parcel",
    "react",
    "reactDom",
    "starter",
    "boilarplate"
  ],
  "author": "Amine Ouldaouali",
  "license": "ISC",
  "dependencies": {
    "gsap": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-router-dom": "latest"
  },
  "devDependencies": {
    "autoprefixer": "latest",
    "parcel-bundler": "latest",
    "postcss-cli": "latest",
    "sass": "latest",
    "tailwindcss": "latest"
  }
}
